{
  "name": "wongames",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src --max-warnings=0",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "schema:download": "apollo client:download-schema --endpoint=http://localhost:1337/graphql schema-temp.gql",
    "schema:convert": "node convertToNonNull.js",
    "codegen": "apollo codegen:generate --target typescript --excludes=node_modules/ --includes=src/graphql/**/*.ts --localSchemaFile schema.gql --tagName=gql --outputFlat src/graphql/generated",
    "generate": "yarn plop --plopfile generators/plopfile.js",
    "types:generate": "yarn schema:download && yarn schema:convert && yarn codegen",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook -s ./public",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e": "start-server-and-test dev http://localhost:3000 cy:open"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*": [
      "yarn test --findRelatedTests --bail"
    ]
  },
  "dependencies": {
    "@stripe/react-stripe-js": "^1.6.0",
    "@stripe/stripe-js": "^1.21.2",
    "graphql": "^15.5.1",
    "joi": "^17.5.0",
    "next": "^12.0.9",
    "next-auth": "^3.29.0",
    "next-pwa": "^5.6.0",
    "next-seo": "^4.28.1",
    "nextjs-progressbar": "^0.0.11",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-slick": "^0.28.1",
    "styled-components": "5.3.0",
    "styled-icons": "^10.32.0",
    "styled-media-query": "^2.1.2"
  },
  "devDependencies": {
    "@apollo/client": "^3.5.7",
    "@babel/core": "^7.15.5",
    "@babel/preset-typescript": "^7.12.7",
    "@jackfranklin/test-data-bot": "^1.4.0",
    "@storybook/addon-essentials": "6.4.9",
    "@storybook/react": "6.4.10",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/jest-dom": "^5.13.0",
    "@testing-library/react": "^12.1.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.4.0",
    "@types/lodash.xor": "^4.5.6",
    "@types/node": "^17.0.16",
    "@types/react": "^17.0.38",
    "@types/react-slick": "^0.23.8",
    "@types/styled-components": "^5.1.19",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.26.1",
    "apollo": "^2.33.9",
    "babel-loader": "^8.2.3",
    "babel-plugin-styled-components": "^1.12.0",
    "cypress": "^9.1.1",
    "cypress-plugin-stripe-elements": "^1.0.2",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.2",
    "jest": "^26.6.3",
    "jest-styled-components": "^7.0.4",
    "lint-staged": "^12.1.4",
    "lodash.xor": "^4.5.0",
    "msw": "^0.36.8",
    "node-fetch": "^3.0.0",
    "plop": "^3.0.5",
    "prettier": "^2.4.1",
    "setimmediate": "^1.0.5",
    "start-server-and-test": "^1.14.0",
    "storybook-addon-next-router": "^2.0.3",
    "typescript": "^4.3.5"
  }
}
